    <Name>Metro for Steam</Name>
    <Description>Metro for Steam brings a fresh, clean style to the platform you already know and love. This is Steam like you've never seen it before.</Description>
    <Website>http://metroforsteam.com</Website>
    <FileDownload>
      <Method>deviantart</Method>
      <DeviantURL>http://boneyardbrew.deviantart.com/art/Metro-for-Steam-292419787</DeviantURL>
      <GithubUseTags>false</GithubUseTags>
      <CreateFolder>false</CreateFolder>
    </FileDownload>
    <RemoteVersionInfo>
      <MatchURL>http://boneyardbrew.deviantart.com/art/Metro-for-Steam-292419787</MatchURL>
      <MatchPattern>&gt;Metro for Steam - (.*)&lt;/a&gt;</MatchPattern>
      <MatchGroup>1</MatchGroup>
    </RemoteVersionInfo>
    <LocalVersionInfo>
      <MatchURL>resource\menus\steam.menu</MatchURL>
      <MatchPattern>text="Metro For Steam - (.*)"</MatchPattern>
      <MatchGroup>1</MatchGroup>
    </LocalVersionInfo>
    <ExtraStuff>
      <FontList />
      <FoldersToDeleteOnInstall />
      <FilesToDeleteOnInstall />
      <FoldersToDeleteOnUpdate />
      <FilesToDeleteOnUpdate />
    </ExtraStuff>
  </CatalogEntry>
